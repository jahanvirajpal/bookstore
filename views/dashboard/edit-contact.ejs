<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css"
 rel="stylesheet">


    <div class="container pb-5 px-3">
        <h2 align="center" class="fw-bolder py-3"> Edit Contact </h2>
    

            <form method="post">
                <input type="text" class="form-control " my-2placeholder="Name :" name="name" value="<%= editcontact.name %>">
                <input type="email" class="form-control my-2" placeholder="Email :" name="email" value="<%= editcontact.email %>">
                <input type="number" class="form-control my-2" placeholder="Mobile :" name="mobile" value="<%= editcontact.mobile %>">
                <input type="text" class="form-control my-2" placeholder="Address :" name="address" value="<%= editcontact.address %>">
                <div class="form-floating">
                <textarea class="form-control my-2" name="comments" id="textarea"><%= editcontact.comments %></textarea>
                <label for="textarea"> Comments :</label></div>
                <input class="form-check-input" type="checkbox"> I agree to continue
           
            <button type="submit" class="btn btn-primary">Update</button>
        </form>

        </div>
